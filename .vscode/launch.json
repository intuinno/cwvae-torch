        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "mmnist",
            "type": "python", 
            "request": "launch", 
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "justMyCode": true, 
            "args": [
                "--configs", "defaults",  "mmnist"
            ]
        },
        {
            "name": "mmnist 1level",
            "type": "python", 
            "request": "launch", 
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "justMyCode": true, 
            "args": [
                "--configs", "defaults",  "mmnist_cont_1level",
                "--exp_name", "debug",
                "--debug", "True",
                "--levels", "1",
                "--device", "cuda:3",
                // "--load_model", "logs/mmnist/3level-discrete_20230412_161938/latest_model.pt"
            ]
        },
        {
            "name": "eval mmnist",
            "type": "python", 
            "request": "launch", 
            "program": "${workspaceFolder}/eval.py",
            "console": "integratedTerminal",
            "justMyCode": true, 
            "args": [
                "--model", "logs/logs/mmnist/3level-discrete_20230412_161938/latest_model.pt",
                "--open-loop-ctx", "16",  
            ]
        }
    ]
